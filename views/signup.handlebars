<body id="home_bg">
<div class="container" >
	<h2 id="sign">Sign up with your UCSD account</h2>
	<!--<form id="login" role="form" method="get" action = "/menu" onsubmit="return signupValidation();" >-->
	<form id="sign_up" role="form" method="post" action="Javascript:signupValidation()">
	    <div class="form-group">
	      <label class="signup_email" for="email">Your UCSD Email:</label>
	      <input type="text" class="form-control" id="emaill" placeholder="Example@ucsd.edu" name="email">
	    </div>
	    <div class="form-group">
	      <label class="password_label" for="password">Password:</label>
	      <input type="password" class="form-control" id="password1" placeholder="Password" name="password">
	    </div>
	    <div class="form-group">
	      <label class="password_label" for="password">Confirm Password:</label>
	      <input type="password" class="form-control" id="password2" placeholder="Password" name="password">
	    </div>
	    <div class="form-group">
	      <button type="submit" class="form-control btn-primary disabled" id="signup_btn" class="btn" value="Sign Up">Sign Up</button>
	    </div>
	</form>

</div>
