		   						{{/each}}
		   						<p class="help-block">{{help}}</p>
							</div>
						{{/if}}
		   			{{/if}}
		   			</td></tr></table>
		    	</div>
		    {{/each}}
		    <div class="rent_buttons">
		    <input class="btn btn-primary sub-form submit" type="submit" id="rent_back" value="Back">
			<input class="btn btn-primary form-control sub-form disabled submit"type="submit" id="rent_save" value="Save">
			<input class="btn btn-primary sub-form submit"type="submit" id="rent_submit" value="Submit">
			</div>
		</fieldset>
		</form>
    </div>

