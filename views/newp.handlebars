	<table class="user_table">
		<tr>
		  <td class="user_name">Hello, User</td>
		  <td>Your Account</td>
		  <td class="logoutgroup logout_sp"><button class="btn btn-default">Log Out</button></td>
		</tr>
	</table>
	<p id="prof-label"><p>
	<p class="main_help_txt">Click on a red block and fill out the form to turn the block green!</p>
	<div id="main_buttons">
		<div class="block" id="b1">
			<button id="type_btn" type="button" class="btn btn-danger btn-lg btn-block main_btn">Type</button>
			<button id="date_btn" type="button" class="btn btn-danger btn-lg btn-block main_btn">Date</button>
		</div>
		<div class="block" id="b1">
			<button id="rent_btn" type="button" class="btn btn-danger btn-lg btn-block main_btn">Rent</button>
			<button id="property_btn" type="button" class="btn btn-danger btn-lg btn-block main_btn">Property</button>
		</div>
		<div class="block" id="b1">
			<button id="address_btn" type="button" class="btn btn-danger btn-lg btn-block main_btn">Address</button>
			<button id="other_btn" type="button" class="btn btn-danger btn-lg btn-block main_btn">Other</button>
		</div>
		<p class="main_help_txt">Submit only after all blocks are green!</p>
		<div id="data">
			  <button type="button" class="btn btn-primary disabled" id="save" >Save</button>
			  <button type="button" class="btn btn-primary" id="ss" >Submit</button>
		</div>
	</div>
	<div id="first_div">
		<form class="well" id="first_form" role="form" method="get" action="">
			<fieldset>
			<legend class="legends">New Profile</legend>
			<div class="form-group control-group" id="pname-group">
			    <label class="control-label form-label" for="pname">Profile Name: </label></br>
			    <div class="controls">
			    	<p class="help-block">Give your profile a short name!</p>
			        <input type="text" placeholder="Profile Name..." class="input-xlarge" id="pname">		        
			    </div>
			</div>
			<div class="form-group control-group">
			    <label class="control-label form-label" for="Profile Type">Profile Type: </label></br>
			    <div class="controls">
			        <input type="radio" class="input-xlarge" value="Rent In" name="Profile Type" checked><strong>Rent In</strong>
			        <span class="help-inline">I want to be a tenant!</span></br>
			        <input type="radio" class="input-xlarge" value="Rent Out" name="Profile Type"><strong>Rent Out</strong>
			        <span class="help-inline">I want to be a landlord!</span>
			    </div>
			</div>
			<div class="first_buttons">
		    	<input type="submit" id="first_back" value="Back" class=" btn btn-primary sub-form form-control">
				<input type="submit" id="first_submit" value="Next" class=" btn btn-primary sub-form form-control">
			</div>
		</fieldset>
		</form>
	</div>
	<div id="type_div">
		<form class="well" id="type_form" role="form" method="get" action="">
			<fieldset>
			<legend class="legends">Type</legend>
			{{#each Types}}
		   		<div class="form-group control-group">
		   		<table class="form-table"><tr><td class="cell label-cell">		
		   			{{#if radio}}
		   				<label class="form-label">{{label}}: </label>
		   				</td><td class="cell elem-cell">
		   				<div class="controls">
		   					{{#each options}}
		   						<input type="radio" name="{{../label}}" value="{{this}}">{{this}}
		   					{{/each}}
		   					<p class="help-block">{{help}}</p>
		   					<div class="clear"></div>
		   				</div>	
		   			{{else}}	
		   				{{#if dropdown}}	
		   					<label class="form-label">{{label}}: </label>
		   					</td><td class="cell">
		   					<div class="controls">	
		   						<select name="{{label}}">		
		   							{{#each options}}
										<option value="{{this}}">{{this}}</option>
									{{/each}}
								</select>
								<p class="help-block">{{help}}</p>
							</div>	
						{{else}}
							<label class="form-label">{{label}}: </label>
							</td><td class="cell">
							<div class="controls">	
		   						{{#each options}}
		   							<input type="checkbox" name="{{../label}}" value="{{this}}">{{this}}<br>
		   						{{/each}}
		   						<p class="help-block">{{help}}</p>
		   					</div>
						{{/if}}
		   			{{/if}}
		   			</td></tr></table>
		    	</div>
		    {{/each}}
		    <div class="type_buttons">
	    	<input class="btn btn-primary sub-form submit" type="submit" id="type_back" value="Back">
			<input class="btn btn-primary form-control sub-form disabled submit" type="submit" id="type_save" value="Save">
			<input class="btn btn-primary sub-form submit" type="submit" id="type_submit" value="Submit">
			</div>
		</fieldset>
		</form>
	</div>
    <div id="rent_div">
