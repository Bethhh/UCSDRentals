	<div id="type_div">
		<form id="type_form" role="form" method="get" action="">
			{{#each Types}}
		   		<div class="form-group">		
		   			{{#if radio}}
		   				<label>{{label}}: </label>
		   				{{#each options}}
		   					<input type="radio" name="{{../label}}" value="{{this}}">{{this}}
		   				{{/each}}
		   			{{else}}	
		   				{{#if dropdown}}	
		   					<label>{{label}}: </label>	
		   					<select name="{{label}}">		
		   						{{#each options}}
									<option value="{{this}}">{{this}}</option>
								{{/each}}
							</select>
						{{else}}
							<label>{{label}}: </label>	
		   					{{#each options}}
		   						<input type="checkbox" name="{{../label}}" value="{{this}}">{{this}}<br>
		   					{{/each}}
						{{/if}}
		   			{{/if}}
		    	</div>
		    {{/each}}
	    	<input type="submit" id="type_back" value="Back" class="submit">
			<input type="submit" id="type_save" value="Save" class="submit">
			<input type="submit" id="type_submit" value="Submit" class="submit">
		</form>
	</div>
