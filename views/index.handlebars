	<form id="login" role="form" method="post" action="Javascript:validateUser()">
	    <div class="form-group">
	    	<div id="emailcontainer"></div>
	      <label class="email_label" for="email">Your UCSD Email:</label>
	      <input type="text" class="form-control" id="email" placeholder="Example@ucsd.edu" name="email">
	    </div>
	    <div class="form-group">
	    	<div id="pwcontainer"></div>
	      <label class="password_label" for="password">Password:</label>
	      <input type="password" class="form-control" id="password" placeholder="Password" name="password">
	    </div>
	    <div class="form-group">
	      <a class="form-control btn-primary disabled" id="signup" class="btn" href='/signup' value="Sign Up">Sign up</a>
	      <button type="submit" class="form-control btn-primary" id="login_btn" class="btn" value="Log In">Log In</button>
	    </div>
	</form>



</div>
