<html>
<head>
	<title>UCSDRentals</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0" >

	<link href="/css/bootstrap.css" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="/css/bootswatch.min.css">
	<link href="/css/index.css" rel="stylesheet">

</head>

<body id="home_bg">
<div class="container" >
	<h1 id="appname">UCSD Rentals</h1>
	<!-- onsubmit="return validateInfo();" -->
	<!--<form id="login" role="form" method="get" action="/menu" onsubmit="return validateInfo();">-->
	<form id="login" role="form" method="post" action="Javascript:validateUser()">
	    <div class="form-group">
	      <label class="email_label" for="email">Your UCSD Email:</label>
	      <input type="text" class="form-control" id="email" placeholder="Example@ucsd.edu" name="email">
	    </div>
	    <div class="form-group">
	      <label class="password_label" for="password">Password:</label>
	      <input type="password" class="form-control" id="password" placeholder="Password" name="password">
	    </div>
	    <div class="form-group">
	      <a class="form-control btn-primary disabled" id="signup" class="btn" href='/signup' value="Sign Up">Sign up</a>
	      <button type="submit" class="form-control btn-primary" id="login_btn" class="btn" value="Log In">Log In</button>
	    </div>
	</form>
	<script src="js/jquery-1.11.0.js"></script>
	<script src="js/handlebars.js"></script>
	<script src="js/ucsdrentals.js"></script>

</div>

</body>
</html>
