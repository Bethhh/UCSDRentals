<!doctype html>

<html>
<head>
	<title>UCSDRentals</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0" user-scalable="no">
	<link href="/css/bootstrap.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.css" rel="stylesheet">
	<link href="/css/index.css" rel="stylesheet">

</head>

<body>
<div class="container">
	<h1 id="appname">UCSDRentals</h1>
	<div class="logo">
		<img id="logo" src="http://upload.wikimedia.org/wikipedia/en/thumb/4/42/UCSD_Seal.svg/200px-UCSD_Seal.svg.png" />
	</div>

	<form id="login" role="form" method="get" action="/menu">
	    <div class="form-group">
	      <input type="text" class="form-control" id="email" placeholder="example@ucsd.edu" name="email">
	    </div>
	    <div class="form-group">
	      <input type="text" class="form-control" id="password" placeholder="Password" name="password">
	    </div>
	    <div class="form-group">
	      <input type="submit" class="form-control" id="login" class="btn" value="LogIn"></input>

	    </div>
	</form>
</div>

</body>
</html>
